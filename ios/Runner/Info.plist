	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Muni-Report Pro needs your location to tag issues and ideas with accurate geographic information.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Muni-Report Pro needs your location to tag issues and ideas with accurate geographic information.</string>
	
	<!-- Camera Permission -->
	<key>NSCameraUsageDescription</key>
	<string>Muni-Report Pro needs access to your camera to take photos of municipal issues.</string>
	
	<!-- Photo Library Permissions -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Muni-Report Pro needs access to your photo library to attach photos to issue reports.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Muni-Report Pro needs permission to save photos to your library.</string>
	
	<!-- Notifications Permission -->
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
	
